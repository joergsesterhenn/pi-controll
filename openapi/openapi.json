{
  "openapi": "3.1.0",
  "info": { "title": "FastAPI", "version": "0.1.0" },
  "paths": {
    "/door": {
      "post": {
        "summary": "Coop Door",
        "operationId": "coop_door_door_post",
        "security": [{ "HTTPBearer": [] }],
        "parameters": [
          {
            "name": "direction",
            "in": "query",
            "required": true,
            "schema": { "type": "string", "title": "Direction" }
          }
        ],
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": { "application/json": { "schema": {} } }
          },
          "422": {
            "description": "Validation Error",
            "content": {
              "application/json": {
                "schema": { "$ref": "#/components/schemas/HTTPValidationError" }
              }
            }
          }
        }
      }
    },
    "/door-state": {
      "get": {
        "summary": "Door State",
        "operationId": "door_state_door_state_get",
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": { "application/json": { "schema": {} } }
          }
        },
        "security": [{ "HTTPBearer": [] }]
      }
    },
    "/lights": {
      "post": {
        "summary": "Lights",
        "operationId": "lights_lights_post",
